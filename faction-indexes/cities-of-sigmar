<script type="text/javascript">
$(document).ready(function(){
	var data = localStorage.ShowFluff;
	if (data == undefined) {data = 'true'; localStorage.ShowFluff = data};

	var StrData; if (data == 'true') {StrData = true} else {StrData = false};
	$('.ShowFluff').prop('checked', StrData);
	ChangeShowFluff();

	$('.ShowFluff').change(function () {
      localStorage.ShowFluff = $(this).prop('checked');
	   ChangeShowFluff();
   });

	function ChangeShowFluff() {
	if (localStorage.ShowFluff == 'true')
		{$('.ShowFluff').css('display', '');}
	else
		{$('.ShowFluff').css('display', 'none');}
	}
	var data = localStorage.ShowPitchedBattleProfile;
	if (data == undefined) {data = 'true'; localStorage.ShowPitchedBattleProfile = data};

	var StrData; if (data == 'true') {StrData = true} else {StrData = false};
	$('.ShowPitchedBattleProfile').prop('checked', StrData);
	ChangeShowPitchedBattleProfile();

	$('.ShowPitchedBattleProfile').change(function () {
      localStorage.ShowPitchedBattleProfile = $(this).prop('checked');
	   ChangeShowPitchedBattleProfile();
   });

	function ChangeShowPitchedBattleProfile() {
	if (localStorage.ShowPitchedBattleProfile == 'true')
		{$('.ShowPitchedBattleProfile').css('display', '');}
	else
		{$('.ShowPitchedBattleProfile').css('display', 'none');}
	}
	var data = localStorage.ShowForgeWorldDatasheets;
	if (data == undefined) {data = 'true'; localStorage.ShowForgeWorldDatasheets = data};

	var StrData; if (data == 'true') {StrData = true} else {StrData = false};
	$('.ShowForgeWorldDatasheets').prop('checked', StrData);

	$('.ShowForgeWorldDatasheets').change(function () {
      localStorage.ShowForgeWorldDatasheets = $(this).prop('checked');
	   FilterSelect(false);
	   FilterApply(false);
   });


var data = localStorage.CoS_storage;
if (data === undefined) {data = ''; localStorage.CoS_storage = data;}
$('.FilterSelect').prop('value', data);
FilterSelect(true);

$('.FilterSelect').change(function () {
	localStorage.CoS_storage = $(this).prop('value');
	$('.FilterSelect').prop('value', $(this).prop('value'));
   FilterSelect(false);
	FilterApply(false)
});

function FilterSelect(IsInit) {
var x = localStorage.CoS_storage;
$('.clFl').show();
var arrFl = [{eCode:"AN"}, {eCode:"GF"}, {eCode:"HH"}, {eCode:"HF"}, {eCode:"HK"}, {eCode:"LC"}, {eCode:"MI"}, {eCode:"PH"}, {eCode:"TE"}];
arrFl.forEach(function(item, i, arr) {
if (item.eCode == x)
	{$('.clFl').not('.'+item.eCode).hide();
	}
});
};
function FilterApply(IsInit) {
	$('#cog').show();
	$('.ctrlSelect').prop('disabled', true);
setTimeout(function(IsInit) {
var clFlags = '';
var comma = '';
var clAllFlags = '.sForgeWorld';
if (localStorage.ShowForgeWorldDatasheets == 'false') {clFlags = clFlags + comma + '.sForgeWorld'};
	$('.clFl').filter(clFlags).hide();
	UpdateTooltipster(IsInit);
	$('.ctrlSelect').prop('disabled', false);
	$('#cog').hide();
})
};
FilterApply(true);

function UpdateTooltipster(IsInit) {
if (!IsInit) 
	{
	 $('.tooltip').tooltipster('destroy');
	 $('.tooltipArmyList').tooltipster('destroy');
	 if ($(window).width() >= 800) {
	$('.no_tooltip').addClass('tooltip').removeClass('no_tooltip');
	sidesArr = ['top', 'bottom', 'left', 'right'];
	}
else {
	sidesArr = ['top', 'bottom'];
	}
;
$('.tooltip_').addClass('tooltip').removeClass('tooltip_');
$('.tooltip:hidden').addClass('tooltip_').removeClass('tooltip');
$('.tooltip').tooltipster({
functionBefore: function(instance, helper){
$.each($.tooltipster.instances(), function(i, instance){
	instance.close();
});
},
plugins: ['sideTip', 'scrollableTip'],
contentCloning: true,
position: sidesArr,
restoration: 'current',
maxWidth: 600,
viewportAware: true,
repositionOnScroll: false,
trigger: 'custom',
triggerOpen: {
    mouseenter: true,
    tap: true
},
triggerClose: {
    click: true,
	 mouseleave: true,
    tap: true
}
});
$('.tooltipArmyList').tooltipster({
functionBefore: function(instance, helper){
$.each($.tooltipster.instances(), function(i, instance){
	instance.close();
});
},
plugins: ['sideTip', 'scrollableTip'],
position: 'left',
restoration: 'current',
viewportAware: true,
arrow: false,
positionTracker: true,
contentCloning: true,
repositionOnScroll: false,
maxWidth: 1100,
trigger: 'custom',
triggerOpen: {
    mouseenter: true,
    tap: true
},
triggerClose: {
    click: true,
	 mouseleave: true,
    tap: true
}
});
	}
};
if ($(window).width() >= 800) {
	$('.no_tooltip').addClass('tooltip').removeClass('no_tooltip');
	sidesArr = ['top', 'bottom', 'left', 'right'];
	}
else {
	sidesArr = ['top', 'bottom'];
	}
;
$('.tooltip_').addClass('tooltip').removeClass('tooltip_');
$('.tooltip:hidden').addClass('tooltip_').removeClass('tooltip');
$('.tooltip').tooltipster({
functionBefore: function(instance, helper){
$.each($.tooltipster.instances(), function(i, instance){
	instance.close();
});
},
plugins: ['sideTip', 'scrollableTip'],
contentCloning: true,
position: sidesArr,
restoration: 'current',
maxWidth: 600,
viewportAware: true,
repositionOnScroll: false,
trigger: 'custom',
triggerOpen: {
    mouseenter: true,
    tap: true
},
triggerClose: {
    click: true,
	 mouseleave: true,
    tap: true
}
});
$('.tooltipArmyList').tooltipster({
functionBefore: function(instance, helper){
$.each($.tooltipster.instances(), function(i, instance){
	instance.close();
});
},
plugins: ['sideTip', 'scrollableTip'],
position: 'left',
restoration: 'current',
viewportAware: true,
arrow: false,
positionTracker: true,
contentCloning: true,
repositionOnScroll: false,
maxWidth: 1100,
trigger: 'custom',
triggerOpen: {
    mouseenter: true,
    tap: true
},
triggerClose: {
    click: true,
	 mouseleave: true,
    tap: true
}
});
});
$.tooltipster.setDefaults({
arrow: true,
delay: [200, 200],
interactive: true,
theme: ['tooltipster-noir', 'tooltipster-noir-customized']
});
</script>
<script type="text/javascript">
var touchHover = function() {
    $('.tooltip').on('touchstart', function(e) {
          e.preventDefault();
          return false;
    });
};
$(document).ready(function() { touchHover(); });
</script>

